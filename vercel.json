{
    "functions": {
        "api/*.php": {
            "runtime": "vercel-php@0.6.0",
            "memory": 1024,
            "maxDuration": 10
        }
    },
    "routes": [
        {
            "src": "/favicon.ico",
            "headers": {
                "Cache-Control": "public, max-age=2629746, immutable"
            },
            "dest": "/public/favicon.ico"
        },
        {
            "src": "/bg.jpeg",
            "headers": {
                "Cache-Control": "public, max-age=2629746, immutable"
            },
            "dest": "/public/bg.jpeg"
        },
        {
            "src": "/cowo.png",
            "headers": {
                "Cache-Control": "public, max-age=2629746, immutable"
            },
            "dest": "/public/cowo.png"
        },
        {
            "src": "/cewe.png",
            "headers": {
                "Cache-Control": "public, max-age=2629746, immutable"
            },
            "dest": "/public/cewe.png"
        },
        {
            "src": "/sound_background.mp3",
            "headers": {
                "Cache-Control": "public, max-age=2629746, immutable"
            },
            "dest": "/public/sound_background.mp3"
        },
        {
            "src": "/robots.txt",
            "headers": {
                "Cache-Control": "public, max-age=2629746, immutable"
            },
            "dest": "/public/robots.txt"
        },
        {
            "src": "/(css|js)/(.*)",
            "headers": {
                "Cache-Control": "public, max-age=86400, immutable"
            },
            "dest": "/public/$1/$2"
        },
        {
            "src": "/(.*)",
            "dest": "/api/index.php"
        }
    ]
}
